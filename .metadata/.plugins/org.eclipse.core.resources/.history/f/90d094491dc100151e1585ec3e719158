package modelo;

public interface Operaciones {

	public static double perimetro(double l1, double base, double altura, double lt1, double lt2, double lt3, double diametro) {
		// TODO Auto-generated method stub
		return l1*4;
	}
	public static double area(double l1, double base, double altura, double lt1, double lt2, double lt3, double diametro) {
		// TODO Auto-generated method stub
		return l1*l1;
	}
	
}
