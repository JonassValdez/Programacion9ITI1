package modelo;

public class Cuadrado implements Operaciones{

	public  double operaciones(double l1) {
		// TODO Auto-generated method stub
		return l1*4;
	}


	public double area(double l1, double base, double altura, double lt1, double lt2, double lt3, double diametro) {
		// TODO Auto-generated method stub
		return l1*l1;
	}

}
